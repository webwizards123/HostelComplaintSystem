<!DOCTYPE html>
<html lang="en">

<style>
    table, th, td ,tr{
        border:1px solid black;
        padding: 20px;
    }
</style>

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <!-- Style sheets CSS and bootstrap -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="styles_student.css">

    <!-- Javascript and jquery -->
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
</head>
<body>
    <nav class="navbar navbar-expand-md navbar-light justify-content-between">
    
    <div class="inst">
        <a class="navbar-brand" href="#"><img class="profile" src="iit1.jfif" alt=""></a>
        IIT BHU
    </div>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav ml-auto ms-auto">
            <li class="nav-item active">
                <div class="right">
                    <a class="nav-link" href="/studentHome">Home</a>
                </div>
            </li>
            <li class="nav-item">
                <div class="right">
                    <a class="nav-link" href="/generate_complaint">Raise Ticket</a>
                </div>
            </li>
            <li class="nav-item">
                <div class="right">
                    <a class="nav-link" href="#">Notice</a>
                </div>
            </li>
        </ul>
        <div class="right">
            <a href="https://www.google.com/" ><img src="profile1.jpg" class="profile" alt=""></a>    
        </div>
    </div>
</nav>
    <br><br><br>
    <table class="styled-table">
        <thead>
            <tr>
                <th>
                    Complaint_id
                </th>
                
                <th>
                    Category
                </th>
                <th>
                    title
                </th>
                
                <th>
                    description
                </th>
                
                <th>
                    student_ref_id
                </th>

                <th>
                    hostel_ref_id
                </th>
                
                <th>
                    status
                </th>
                
                <th>
                    room_no
                </th>
                <th>
                    Acknowledgement
                </th>
            </tr>
        </thead>
        
        <tbody>
            <% arr.forEach(function(ele){ %>
                <tr>
                    <td><%= ele.complaint_id %></td>
                    <td><%= ele.category %></td>
                    <td><%= ele.title %></td>
                    <td><%= ele.description %></td>
                    <td><%= ele.student_ref_id %></td>
                    <td><%= ele.hostel_ref_id %></td>
                    <td><%= ele.status %></td>
                    <td><%= ele.room_no %></td>
                    <td>
                        <form action="/view_my_complaints" method="post">
                            <button type="submit" name="ack" value=<%=ele.complaint_id%> >Acknowledge</button>
                        </form>
                    </td>
                </tr>
            <% }) %>
        </tbody>

    </table>
   
</body>
</html>